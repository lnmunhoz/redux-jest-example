{
  "name": "redux-jest-example",
  "version": "1.0.0",
  "description": "The goal of this repository is to give an example of how to write tests for your reducers using jest.",
  "main": "index.js",
  "dependencies": {
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.22.0",
    "node-uuid": "^1.4.7",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.2"
  },
  "devDependencies": {
    "coveralls": "^2.11.16",
    "expect": "^1.20.2"
  },
  "jest": {
    "collectCoverage": true
  },
  "scripts": {
    "test": "jest --coverage && npm run coveralls",
    "test:watch": "npm test --watch",
    "coveralls": "cat coverage/lcov.info | node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lnmunhoz/redux-jest-example.git"
  },
  "keywords": [
    "redux",
    "jest",
    "test",
    "example"
  ],
  "author": "Lucas N. Munhoz <ln.munhoz@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lnmunhoz/redux-jest-example/issues"
  },
  "homepage": "https://github.com/lnmunhoz/redux-jest-example#readme"
}
